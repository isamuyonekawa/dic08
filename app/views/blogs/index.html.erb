<h1>ブログ一覧</h1>

<p><%= notice %></p>

<table>
  <tr>
      <th>タイトル</th>
      <th>内容</th>
  </tr>
  
<% @blogs.each do |blog| %>
  <tr class=index>
      <td><%= blog.title %></td>
      <td><%= blog.content %></td>
      <td><%= link_to '詳細', blog_path(blog.id) %></td>
  </tr>
<% end %>
</table>

<%= link_to '新規作成', new_blog_path %>


